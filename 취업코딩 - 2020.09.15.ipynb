   "source": [
    "import sys\n",
    "\n",
    "N, K = map(int, input().split())\n",
    "cnt = 0\n",
    "while True:\n",
    "    if N == 1:\n",
    "        print(cnt)\n",
    "        break\n",
    "    else:\n",
    "        if N % K == 0:\n",
    "            N = N // K\n",
    "            cnt += 1\n",
    "        else:\n",
    "            N = N-1\n",
    "            cnt +=1"
   ]
